{
    "entryFile": "src/app.ts",
    "controllerPathGlobs": [
        "src/**/*Controller.ts"
    ],
    "routes": {
        "basePath": "/api",
        "entryFile": "./src/app.ts",
        "routesDir": "./src"
    },
    "spec": {
        "basePath": "/api",
        "outputDirectory": "./src",
        "specVersion": 3,
        "description": "Restful API for Wealthcare app",
        "schemes": ["http"],
        "name": "Wealthcare API"

    },
    "swagger": {
        "outputDirectory": ".",
        "entryFile": "./src/app.ts"
    }
}